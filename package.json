{
  "name": "next-react-app",
  "version": "2.0.0",
  "type": "module",
  "description": "Personal Next React App",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navin-moorthy/next-react-app.git"
  },
  "bugs": {
    "url": "https://github.com/navin-moorthy/next-react-app/issues"
  },
  "author": "Navin Moorthy <navin007.a@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "react",
    "next-react-app",
    "typescript"
  ],
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true pnpm build && pnpm start",
    "build:ci": "SKIP_ENV_VALIDATION=true next build",
    "build:sourcemap": "SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
    "build:start": "pnpm build && pnpm start",
    "codemod:dev": "NODE_OPTIONS=\"--experimental-specifier-resolution=node\" nodemon --watch './codemods/**/*' -e .js,.cjs ./codemods/main.js",
    "codemod:main": "NODE_OPTIONS=\"--experimental-specifier-resolution=node\" node ./codemods/main.js",
    "codemod:staging": "NODE_OPTIONS=\"--experimental-specifier-resolution=node\" node ./codemods/staging.js",
    "commit": "gacp",
    "dev": "next dev",
    "dev:sourcemap": "SOURCEMAP=true pnpm dev",
    "format": "npm-run-all fix:*",
    "format:csslint": "stylelint '**/*.css' --fix --cache --color --ignore-path .prettierignore -f verbose",
    "format:eslint": "eslint --cache --color --ext .js,.cjs,.jsx,.ts,.cts,.tsx . --fix",
    "format:prettier": "prettier --write --cache --check \"./**/*.{html,css,js,cjs,jsx,ts,cts,tsx,md,json}\"",
    "lint": "npm-run-all lint:*",
    "lint:csslint": "stylelint '**/*.css' --cache --color --ignore-path .prettierignore -f verbose",
    "lint:eslint": "eslint --cache --color --ext .js,.cjs,.jsx,.ts,.cts,.tsx .",
    "lint:prettier": "prettier -l --cache \"./**/*.{html,css,js,cjs,jsx,ts,cts,tsx,md,json}\"",
    "lint:types": "tsc --noEmit",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "postinstall": "husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "release": "release-it --no-npm",
    "release:dryrun": "release-it --dry-run",
    "start": "next start",
    "test": "jest",
    "test:ci": "SKIP_ENV_VALIDATION=true jest --ci"
  },
  "dependencies": {
    "@adaptui/react": "1.0.0-alpha.9",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-slider": "^1.0.0",
    "@react-aria/dnd": "^3.0.1",
    "@react-aria/utils": "^3.14.2",
    "@stitches/react": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.5.2",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.1.3",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "allotment": "^1.17.0",
    "ariakit": "^2.0.0-next.41",
    "axios": "^0.27.2",
    "base64-arraybuffer": "^1.0.2",
    "chrome-aws-lambda": "^10.1.0",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "cypress": "^12.5.1",
    "date-fns": "^2.29.3",
    "filereader": "^0.10.3",
    "heroicons": "^1.0.6",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "13.1.6",
    "next-auth": "^4.7.0",
    "next-seo": "5.15.0",
    "nodemailer": "^6.7.8",
    "npm": "^8.14.0",
    "puppeteer-core": "^15.4.1",
    "react": "18.2.0",
    "react-aria": "^3.22.0",
    "react-avatar": "^5.0.3",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-masonry-css": "^1.0.16",
    "react-mentions": "^4.4.7",
    "react-select": "^5.4.0",
    "react-spinners": "^0.13.4",
    "react-stately": "^3.20.0",
    "react-toastify": "^9.0.8",
    "slugify": "^1.6.5",
    "tailwind-merge": "^1.6.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@ianvs/prettier-plugin-sort-imports": "3.7.1",
    "@next/bundle-analyzer": "13.1.6",
    "@next/env": "13.1.6",
    "@next/eslint-plugin-next": "13.1.6",
    "@release-it/conventional-changelog": "5.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-mentions": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "^3.8.2",
    "gacp": "3.0.3",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "lint-staged": "13.1.0",
    "next-bundle-analyzer": "0.6.7",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "3.1.48",
    "npm-run-all": "^4.1.5",
    "pinst": "3.0.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-pkg": "0.17.1",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "release-it": "15.6.0",
    "sharp": "0.31.3",
    "stylelint": "14.16.1",
    "stylelint-config-clean-order": "^2.3.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "tailwindcss": "3.2.4",
    "tilg": "0.1.1",
    "typescript": "4.9.4",
    "zod": "3.20.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "gacp": {
    "add": false,
    "push": false,
    "emoji": "emoji",
    "editor": false
  },
  "lint-staged": {
    "**/*.{js,cjs,jsx,ts,cts,tsx}": [
      "pnpm lint:eslint"
    ],
    "**/*.{html,css,js,cjs,jsx,ts,cts,tsx,md,json}": [
      "pnpm lint:prettier"
    ]
  }
}
