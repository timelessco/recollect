{
	"name": "recollect",
	"version": "0.0.0",
	"private": true,
	"description": "Bookmarks is an open source url manager built using nextjs and supabase.",
	"keywords": [
		"next",
		"react",
		"typescript",
		"tailwind",
		"supabase",
		"react-query"
	],
	"bugs": {
		"url": "https://github.com/timelessco/recollect/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/recollect.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"build": "npx turbo run build:next",
		"postbuild": "next-sitemap --config next-sitemap.config.cjs",
		"build:analyze": "ANALYZE=true pnpm build && pnpm start",
		"build:ci": "SKIP_ENV_VALIDATION=true next build",
		"build:next": "next build",
		"build:sourcemap": "SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"build:start": "pnpm build && pnpm start",
		"check:engine": "npx installed-check --fix",
		"check:packages": "pnpm dedupe --check",
		"check:update": "npx npm-check-updates --interactive --format group --target patch",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate",
		"cypress:open": "cypress open",
		"dev": "next dev",
		"dev:sourcemap": "SOURCEMAP=true pnpm dev",
		"fix": "turbo fix:eslint",
		"fix:css": "stylelint --fix --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"fix:eslint": "eslint --cache --color --fix .",
		"fix:md": "markdownlint-cli2 --fix --rules sentences-per-line",
		"fix:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"fix:spelling": "tsx scripts/update-cspell-words.ts",
		"lint": "turbo lint:eslint lint:types lint:css lint:md lint:knip lint:spelling lint:prettier",
		"lint:css": "stylelint --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"lint:eslint": "eslint --cache --color .",
		"lint:knip": "knip --production",
		"lint:md": "markdownlint-cli2 --rules sentences-per-line",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:spelling": "cspell --gitignore --cache --no-progress --no-summary --show-context --dot .",
		"lint:types": "tsc --noEmit",
		"prepare": "is-ci || husky",
		"release": "node release-it/getCommitsSinceLastRelease.js && release-it --ci",
		"release:dryrun": "release-it --dry-run",
		"start": "next start",
		"test": "echo \"no test specified\" && exit 0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"tsx scripts/update-cspell-words.ts",
			"prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown"
		]
	},
	"dependencies": {
		"@adaptui/react": "1.0.0-alpha.9",
		"@ariakit/react": "0.3.7",
		"@ariakit/react-core": "0.3.7",
		"@aws-sdk/client-s3": "3.840.0",
		"@aws-sdk/s3-request-presigner": "3.840.0",
		"@google/generative-ai": "0.24.1",
		"@headlessui/react": "1.7.13",
		"@heroicons/react": "1.0.6",
		"@supabase/ssr": "0.3.0",
		"@supabase/supabase-js": "2.57.4",
		"@tanstack/react-query": "4.28.0",
		"@tanstack/react-virtual": "3.13.12",
		"allotment": "1.20.4",
		"ariakit": "2.0.0-next.43",
		"array-to-image": "1.0.0",
		"axios": "1.13.1",
		"base64-arraybuffer": "1.0.2",
		"blurhash": "2.0.5",
