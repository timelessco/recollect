		"sharp": "0.32.6",
		"slugify": "1.6.6",
		"tailwind-merge": "1.12.0",
		"uniqid": "5.4.0",
		"yet-another-react-lightbox": "3.23.3",
		"zod": "3.22.3",
		"zustand": "4.3.7"
	},
	"devDependencies": {
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
